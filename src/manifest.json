{
  "manifest_version": 2,

  "name": "Chromespaces",
  "description": "Chrome workspaces",
  "version": "0.2",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["js/jquery.min.js", "js/background.js"]
  },

  "browser_action": {
    "default_icon": "images/inactive.png",
    "default_popup": "chromespaces.html"
  }
}