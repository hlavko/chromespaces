{
  "manifest_version": 2,

  "name": "Chromespaces",
  "description": "Chrome workspaces",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}